<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Dashboard | Pictures</title>
  <link rel="stylesheet" href="/vendor/materialize.min.css">
  <script src="/vendor/jquery-3.5.1.min.js"></script>
  <script src="/vendor/materialize.min.js"></script>
  <script src="/js/sidenav.js"></script>
  <script src="/js/business/pictures.js"></script>
  <link rel="stylesheet" href="/css/dashboard.css">
  <script src="/js/business/dash-nav.js"></script>
  <link rel="stylesheet" href="/css/pictures.css">
</head>
<body>
  <main class="row">
    <div class="col s12 m4 l2 blue-grey lighten-2" id="dashboard-nav">
      <%- include ('../partials/dash-nav.ejs') %> 
    </div>
    <div class="col s12 m8 l10 grey lighten-5" id="dashboard-content">
      <div class="container">
        <h2 class="center dashboard-title">Pictures</h2>
        <div class="divider"></div>
        <div class="divider"></div>

        <div class="section"></div>

        <div class="row" id="pictures-box">

        </div>
        
        <div class="section"></div>

        <div class="divider"></div>
        <div class="divider"></div>
        <h4 class="center">Add Picture</h4>
    
        <div class="row">
          <div class="col s8">
            <form enctype="multipart/form-data">
              <div class="file-field input-field">
                <div class="btn">
                  <span>Choose file</span>
                  <input name="file" type="file" id="file">
                </div>
                <div class="file-path-wrapper">
                  <input class="file-path validate" type="text" id="file-text">
                </div>
              </div>
            </form>
          </div>
          <div class="col s2">
            <div class="file-field input-field">
              <a class="waves-effect waves-light btn" id="upload-btn">Upload</a>
            </div>
          </div>
        </div>

        <!-- Page padding -->
        <div class="section"></div>
        <div class="section"></div>
        <div class="section"></div>
        <div class="section"></div>
        <div class="section"></div>
        <div class="section"></div>
      </div>
    </div>
  </main>
</body>
</html>